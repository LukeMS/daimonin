Object ai_caster
name Spellcaster AI
type 126
msg
# This is a generic Spellcaster AI that can be used for most mobs
processes:
look_for_other_mobs
friendship
choose_enemy

moves: 
#stand_still
sleep
# run_away_from_enemy hp_threshold=6
# move_towards_enemy
keep_distance_to_enemy
optimize_line_of_fire
move_towards_enemy_last_known_pos
search_for_lost_enemy
move_towards_waypoint
# move_randomly xlimit=5 ylimit=5
move_towards_home

actions:
melee_attack_enemy
bow_attack_enemy
spell_attack_enemy
endmsg
editor_folder ai
face ai.101
sys_object 1
end
