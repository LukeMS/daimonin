<section autotoc="yes"><title>APPENDIX A: GAME COMMANDS</title>
    <section id="movement"><title>Movement</title>
        <table>
            <tr>
                <th>Game Command
                </th>
                <th>Effect
                </th>
            </tr>
            <tr>
                <td>/run
                </td>
                <td>Switch on run mode
                </td>
            </tr>
            <tr>
                <td>/run_stop
                </td>
                <td>Switch off run mode
                </td>
            </tr>
        </table>
    </section>
    <section id="skills"><title>Skills</title>
        <table>
            <tr>
                <th>Game Command
                </th>
                <th>Effect
                </th>
            </tr>
            <tr>
                <td>/ready_skill &lt;skill&gt;
