material GUI/Window
{
   technique
   {
      pass
      {
         lighting off
         scene_blend alpha_blend
	 alpha_rejection greater_equal 128
         depth_check off
	 depth_write off
         texture_unit
         {
	    filtering none
         }
      }
   }
}

material GUI/LoadScreen1
{
  technique
  {
    pass
    {
      lighting off
      scene_blend add
      depth_write off

      texture_unit
      {
        texture loading1.jpg
      }
    }
  }
}

