material Wall/Wall_Brick
{
    technique
    {
     pass
     {
      texture_unit
      {
       texture_alias 0
       texture Wall_Brick.png
      }
     }

    }
}

material Wall/Wall_Brick_Window
{
 technique
 {
  pass
  {
   scene_blend alpha_blend
   depth_write off
   cull_hardware none
   cull_software none
   texture_unit
   {
   alpha_op_ex modulate src_texture src_manual 0.5

    texture Wall_Brick.png
   }
  }
 }
}
