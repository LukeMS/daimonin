Object ai_caster
editor_folder ai/ai
name Spellcaster AI
msg
# This is a generic Spellcaster AI that can be used for most mobs
processes:
look_for_other_mobs
friendship
choose_enemy

moves: 
#stand_still
sleep
# run_away_from_enemy hp_threshold=6
# move_towards_enemy
keep_distance_to_enemy
optimize_line_of_fire
move_towards_enemy_last_known_pos
search_for_lost_enemy
move_towards_waypoint
# move_randomly xlimit=5 ylimit=5
move_towards_home

actions:
melee_attack_enemy
bow_attack_enemy
spell_attack_enemy
endmsg
face ai.101
sys_object 1
type 126
end
Object ai_dog
editor_folder ai/race_defaults
name Dog AI
race dog
msg
# This is an AI for dogs, with a twist (or two)
processes:
look_for_other_mobs
look_for_objects
friendship
choose_enemy
attraction arch=lantern_magic.131:100 type=6:150 cursedtype=6:-300
# Food is type 6

moves: 
sleep
move_towards_enemy
move_towards_enemy_last_known_pos
avoid_repulsive_items
search_for_lost_enemy
move_towards_waypoint
investigate_attraction
move_randomly xlimit=5 ylimit=5
move_towards_home

actions:
melee_attack_enemy
endmsg
face ai.101
sys_object 1
type 126
end
