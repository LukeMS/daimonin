<?xml version="1.0" encoding="us-ascii"?>
<XML ID="GUI"> 
  
  <Cursor ID ="MouseCursor" />

  <Window ID="PlayerInfo" RelativeCoords="false">
    <Pos      X="50" Y="50" Z-Order ="50" />
    <Size     Width="300" Height="300" />
    <DragArea Movable="false" X1="150" Y1="50" X2="1" Y2="1" />
    <Tooltip  Text="This is the Player Info Window." />

    <Gadget Type="BUTTON"  ID ="Button_Close">
      <Pos X="50" Y="50" />
      <Label XPos="5" YPos= "1" Red="32" Green="64" Blue="128" Text="Button"/>
    </Gadget>

    <Graphic Type="GFX_FILL"  ID ="Background">
      <Pos X="10" Y="10" />
      <Range Width="200" Height="150" />
    </Graphic>

    <TextLine X="60" Y="70" Size="40" Text="1234567890" />
    <TextLine X="60" Y="80" Size="40" Text="ABCDEFGHUJ" />
  </Window>


  <Window ID="Quickslots" RelativeCoords="false">
    <Pos      X="250" Y="250" Z-Order ="50" />
    <Size     Width="100" Height="100" />
    <Tooltip  Text="Tjghjghjghjghjghjghj Window." />

    <Gadget Type="BUTTON"  ID ="Button_Close">
      <Pos X="20" Y="20" />
      <Label XPos="5" YPos= "1" Red="32" Green="64" Blue="128" Text="Press"/>
    </Gadget>

    <Graphic Type="COLOR_FILL"  ID ="This is a Color Fill demo">
      <Pos X="0" Y="0" />
      <Color Red="0" Green="0" Blue="255" Alpha ="255" />
      <Range Width="100" Height="100" />
    </Graphic>

  </Window>


</XML>
