# name of the PNG that contains all the images
imageset.filename=Imageset.png

# name of the XML that contains the images meta information
imageset.filename.xml=GUI_ImageSet.xml

# limit the enclosing image size, specified by 'imageset.maxWidth' and 'imageset.maxHeight'
imageset.restrictSize=true

# NOTE: for best packing results chose same values for maxWidth and maxHeight
# imageset maximum width
imageset.maxWidth=768
# imageset maximum height
imageset.maxHeight=768

# top directory containing the single PNG images
images.dir=gfx_source
